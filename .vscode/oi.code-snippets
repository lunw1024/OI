		  "    Mat out(N, vector<mint>(N));",
		  "    for (int i = 0; i < N; i++)",
		  "        out[i][i] = 1;",
		  "    while (b > 0) {",
		  "        if (b & 1) {",
		  "            out = out * a;",
		  "        }",
		  "        b >>= 1;",
		  "        a = a * a;",
		  "    }",
		  "    return out;",
