		  "    ll out = 1;",
		  "    while (x > 0) {",
		  "        if (x & 1) {",
		  "            out = out * a % MOD;",
		  "        }",
		  "        a = a * a % MOD;",
		  "        x >>= 1;",
		  "    }",
		  "    return out;",
		  "}",
		  "",
		  "ll inv(ll x) {",
		  "    return power(x, MOD - 2);",
