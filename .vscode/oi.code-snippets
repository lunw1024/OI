	"Binary Indexed Tree": {
		"prefix": "BIT",
		"body": [
			"struct BIT {",
			"    int n;",
			"    vector<int> data;",
			"    BIT(int n) {",
			"        this->n = n;",
			"        data.resize(n);",
			"    }",
			"    inline int lowbit(int x) {",
			"        return x & -x;",
			"    }",
			"    int query(int x) {",
			"        int out = 0;",
			"        while (x > 0) {",
			"            out += data[x];",
			"            x -= lowbit(x);",
			"        }",
			"        return out;",
			"    }",
			"    void add(int x, int k) {",
			"        while (x < n) {",
			"            data[x] += k;",
			"            x += lowbit(x);",
			"        }",
			"    }",
			"};",
		]
