	"Union Find Set": {
		"prefix": "UnionFindSet",
		"body": [
			"struct UnionFindSet{",
			"    int n;",
			"    vector<int> fa;",
			"    UnionFindSet(int n) {",
			"        this->n = n;",
			"        fa.resize(n);",
			"        for (int i = 0; i < n; i++)",
			"            fa[i] = i;",
			"    }",
			"",
			"    int find(int x) {",
			"        return fa[x] == x ? x : fa[x] = find(fa[x]);",
			"    }",
			"",
			"    void merge(int x, int y) {",
			"        x = find(x), y = find(y);",
			"        fa[x] = y;",
			"    }",
			"};"
		],
	},
	"Euler Sieve": {
		"prefix": "Sieve",
		"body": [
			"struct Sieve {",
			"    int n;",
			"    vector<int> isprime, prime;",
			"    Sieve(int n) {",
			"        this->n = n;",
			"        isprime.resize(n, true);",
			"        for (int i = 2; i < n; i++) {",
			"            if (isprime[i]) {",
			"                prime.push_back(i);",
			"            }",
			"            for (int j : prime) {",
			"                if (1ll * i * j >= n)",
			"                    break;",
			"                isprime[i * j] = false;",
			"                if (i % j == 0)",
			"                    break;",
			"            }",
			"        }",
			"    }",
			"};"
		],
		"description": "Euler Sieve"
